# FlareFlow.link Environment Variables

# LiteLLM Configuration
NEXT_PUBLIC_LITELLM_URL=https://llm.p10p.io
NEXT_PUBLIC_LITELLM_ADMIN_KEY=your_litellm_admin_key_here

# Flow Blockchain Configuration
NEXT_PUBLIC_FLOW_NETWORK=testnet
NEXT_PUBLIC_FLOW_ACCESS_NODE=https://rest-testnet.onflow.org

# Flare Oracle Configuration
NEXT_PUBLIC_FLARE_NETWORK=coston2
NEXT_PUBLIC_FLARE_RPC_URL=https://coston2-api.flare.network/ext/C/rpc

# Application Configuration
NEXT_PUBLIC_APP_NAME=FlareFlow.link
NEXT_PUBLIC_APP_URL=https://flareflow.link

# Development Settings
NODE_ENV=development
NEXT_PUBLIC_DEBUG=false